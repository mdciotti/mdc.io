---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Maxwell Ciotti</title>
	<link href="http://www.mdc.io/atom.xml" rel="self"/>
	<link href="http://www.mdc.io/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.mdc.io/</id>
	<author>
		<name>Maxwell Ciotti</name>
		<email>max@mdc.io</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		<link href="http://www.mdc.io{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://www.mdc.io{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
	
</feed>
